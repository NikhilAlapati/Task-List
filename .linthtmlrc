{
	"maxerr": false,
	"raw-ignore-regex": false,
	"attr-bans": [
		"align",
		"background",
		"bgcolor",
		"border",
		"frameborder",
		"longdesc",
		"marginwidth",
		"marginheight",
		"scrolling",
		"style",
		"width",
		"height"
	],
	"indent-style": "spaces",
	"indent-width": 2,
	"spec-char-escape": true,
	"text-ignore-regex": "((http[s]?|ftp):\/)?\/?([^:\/\\s]+)((\/\\w+)*\/)([\\w\\-\\.]+[^#?\\s]+)(.*)?(#[\\w\\-]+)?",
	"tag-bans": [
		"style",
		"b",
		"i",
		"u",
		"s",
		"tt",
		"big",
		"acronym",
		"dir",
		"frame",
		"frameset",
		"isindex",
		"strike",
		"font",
		"center",
		"applet",
		"basefont",
		"menu"
	],
	"tag-close": true,
	"tag-name-lowercase": true,
	"tag-name-match": true,
	"tag-self-close": "never",
	"doctype-first": true,
	"doctype-html5": true,
	"attr-name-style": "dash",
	"attr-name-ignore-regex": false,
	"attr-no-dup": true,
	"attr-no-unsafe-char": true,
	"attr-order": false,
	"attr-quote-style": "quoted",
	"attr-req-value": true,
	"attr-new-line": false,
	"attr-validate": true,
	"id-no-dup": true,
	"id-class-no-ad": false,
	"id-class-style": "dash",
	"class-no-dup": true,
	"class-style": "dash",
	"id-class-ignore-regex": false,
	"img-req-alt": true,
	"img-req-src": true,
	"html-valid-content-model": true,
	"head-valid-content-model": true,
	"href-style": false,
	"link-req-noopener": false,
	"label-req-for": true,
	"line-end-style": false,
	"line-no-trailing-whitespace": true,
	"line-max-len": 100,
	"line-max-len-ignore-regex": "(href|src)",
	"head-req-title": true,
	"title-no-dup": true,
	"title-max-len": 60,
	"html-req-lang": false,
	"lang-style": "case",
	"fig-req-figcaption": false,
	"focusable-tabindex-style": false,
	"input-radio-req-name": true,
	"input-req-label": false,
	"table-req-caption": false,
	"table-req-header": false,
	"tag-req-attr": false
}